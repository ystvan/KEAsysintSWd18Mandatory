@model KEAWebApp.Models.ViewModels.ShowDataViewModel


<br />
<div class="container">
    <div id="renderTable">
        <table class="table table-bordered table-responsive table-hover">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(g => g.Datas.FirstOrDefault().Location)</th>
                    <th>@Html.DisplayNameFor(g => g.Datas.FirstOrDefault().Light)</th>
                    <th>@Html.DisplayNameFor(g => g.Datas.FirstOrDefault().Temperature)</th>
                    <th>@Html.DisplayNameFor(g => g.Datas.FirstOrDefault().Recorded)</th>


                </tr>

            </thead>
            <tbody>
                @foreach (var data in Model.Datas)
                {
                <tr>
                    <td>@Html.DisplayFor(modelItem => data.Location)</td>
                    <td>@Html.DisplayFor(modelItem => data.Light)</td>
                    <td>@Html.DisplayFor(modelItem => data.Temperature)</td>
                    <td>@Html.DisplayFor(modelItem => data.Recorded)</td>

                </tr>
                }
            </tbody>
        </table>

        <br />
        <br />
        <br />

    </div>
    <button class="btn btn-default">
        @Html.ActionLink("Go back to Home", "Index", "Home")
    </button>


</div>
